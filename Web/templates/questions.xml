{% for section in sections %}
<section id="{{section.sectionID}}">
	{% for question in section.questions %}
	<question id="{{question.questionID}}" type="{{question.type}}" description="{{question.description}}">
		{% for choice in question.choices %}
		<choice value="{{choice.value}}" order="{{choice.order}}" />
		{% endfor %}
	</question>
	{% endfor %}
</section>
{% endfor %}